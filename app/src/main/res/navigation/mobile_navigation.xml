    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/info">

    <fragment
        android:id="@+id/info"
        android:name="com.kautsar.myapplication.ui.info.InfoFragment"
